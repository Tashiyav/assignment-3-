import React, {useState} from "react";
import {SafeAreaView, View, Text, StyleSheet, Button} from 'react-native';
import {Picker} from '@react-native-picker/picker';

export default function App() {
  const [picker1SelectedValue, setPicker1SelectedValue] = useState("Carrots - $5"); 
  const [picker2selectedValue, setPicker2SelectedValue] = useState("1");
  const [calculatedValue, setCalculatedValue] = useState("Press the above button to calculate");
  return (
  <SafeAreaView style={styles.container}>
    <View style ={styles.layer1}> 
      <Text style={styles.heading1}>
        Welcome to the Produce Onwer Group 
      </Text> 
    </View> 
 <View style={styles.layer2}> 
  <Picker
    style={styles.picker1}
    selectedValue={picker1SelectedValue}
    onValueChange={(itemValue) => setPicker1SelectedValue(itemValue)}
  > 
    <Picker.Item label="Carrots - $5" value="5"/>
    <Picker.Item label="Beans - $8" value="8"/>
    <Picker.Item label="Corn - $6" value="6"/>
    <Picker.Item label="Lettuce - $9" value="9"/>
    <Picker.Item label="Tomato - $6" value="6"/>
  </Picker>
  <Picker
    style={styles.picker2}
    selectedValue={picker2selectedValue}
    onValueChange={(itemValue) => setPicker2SelectedValue(itemValue)}
  >
    <Picker.Item label="1" value="1"/>
    <Picker.Item label="2" value="2"/>
    <Picker.Item label="3" value="3"/>
    <Picker.Item label="4" value="4"/>
    <Picker.Item label="5" value="5"/>
  </Picker>
</View>
<View style={styles.layer2}> 
  <Picker
    style={styles.picker1}
    selectedValue={picker1SelectedValue}
    onValueChange={(itemValue) => setPicker1SelectedValue(itemValue)}
  >
    <Picker.Item label="Apples - $10" value="10"/>
    <Picker.Item label="Mango - $13" value="13"/>
    <Picker.Item label="Raspberry - $12" value="12"/>
    <Picker.Item label="Peach - $14" value="14"/>
    <Picker.Item label="Pear - $10" value="10"/>
  </Picker>
  <Picker
    style={styles.picker2}
    selectedValue={picker2selectedValue}
    onValueChange={(itemValue) => setPicker2SelectedValue(itemValue)}
  >
    <Picker.Item label="1" value="1"/>
    <Picker.Item label="2" value="2"/>
    <Picker.Item label="3" value="3"/>
    <Picker.Item label="4" value="4"/>
    <Picker.Item label="5" value="5"/>
  </Picker>
</View>
<View style={styles.layer3}> 
  <Button 
    title="CALCULATOR"
    onPress={() => {
      const intPicker1Value = parseInt(picker1SelectedValue);
      const intPicker2Value = parseInt(picker2selectedValue);
      setCalculatedValue(intPicker1Value * intPicker2Value);
    }}
  />
  <Text style={styles.heading}> The total cost is: $ {calculatedValue} </Text>
</View>

</SafeAreaView>
);
}

  const styles = StyleSheet.create({
    container: {
      flex: 1,
      backgroundColor: '#E3D3E3'
    },
    layer1:{
      paddingTop:10, 
    },
    heading1:{ 
      fontSize: 20, 
      textAlign: 'center', 
      marginTop: 50, 
    }, 
    layer2:{ 
      flexDirection:'row', 
      padding:'250' , 
    }, 
    picker1: { 
      flex: 3, 
    }, 
    picker2:{ 
      flex: 2, 
    }, 
    layer3:{ 
      paddingTop:70, 
    }, 
    heading2:{ 
      marginTop: 30,
      textAlign: 'center',
      colour: '#67e487',
      fontSize: 20,
      fontWeight: 10,
    }
    });








import React, {useState} from "react";
import {SafeAreaView, View, Text, StyleSheet, Button} from 'react-native';
import {Picker} from '@react-native-picker/picker';

export default function App() {
  const [picker1SelectedValue, setPicker1SelectedValue] = useState("Carrots - $5"); 
  const [picker2selectedValue, setPicker2SelectedValue] = useState("1");
  const [calculatedValue, setCalculatedValue] = useState("Press the above button to calculate");
  return (
  <SafeAreaView style={styles.container}>
    <View style ={styles.layer1}> 
      <Text style={styles.heading1}>
        Welcome to the Produce Onwer Group 
      </Text> 
    </View> 
  <View style={styles.layer2}> 
    <Picker style={styles.picker1}> 
      <Picker.Item label="Carrots - $5" value="val-Carrots - $5"/>
      <Picker.Item label="Beans - $8" value="val-Beans - $8"/>
      <Picker.Item label="Corn - $6" value="val-Corn - $6"/>
      <Picker.Item label="Lettuce - $9" value="val-Lettuce - $9"/>
      <Picker.Item label="Tomato - $6" value="val-Tomato - $6"/>
    </Picker>
    <Picker style={styles.picker2}> 
        <Picker.Item label="1" value="val-1"/>
        <Picker.Item label="2" value="val-2"/>
        <Picker.Item label="3" value="val-3"/>
        <Picker.Item label="4" value="val-4"/>
        <Picker.Item label="5" value="val-5"/>
      </Picker>
    </View>
  <View style={styles.layer2}> 
    <Picker style={styles.picker1}> 
      <Picker.Item label="Apples - $10" value="val-Apples - $10"/>
      <Picker.Item label="Mango - $13" value="val-Mango - $13"/>
      <Picker.Item label="Raspberry - $12" value="val-Raspberry - $12"/>
      <Picker.Item label="Peach - $14" value="val-Peach - $14"/>
      <Picker.Item label="Pear - $10" value="val-Pear - $10"/>
    </Picker>
    <Picker style={styles.picker2}>   
        <Picker.Item label="1" value="val-1"/>
        <Picker.Item label="2" value="val-2"/>
        <Picker.Item label="3" value="val-3"/>
        <Picker.Item label="4" value="val-4"/>
        <Picker.Item label="5" value="val-5"/>
      </Picker>
    </View>
    <View style = {styles.layer3}> 
<Button 
  title = "CALCULATOR"
  onPress={() => {
    const lastChar = picker1SelectedValue[picker1SelectedValue.length - 1];
    const charToIntForPicker1 = parseInt(lastChar);
    const charToIntForPicker2 = parseInt(picker2selectedValue);
     setCalculatedValue(charToIntForPicker1 * charToIntForPicker2); 
     }}
    />

    <Text style={styles.heading}> The total cost is:$ {calculatedValue} </Text>
  </View>
</SafeAreaView>
);
}

  const styles = StyleSheet.create({
    container: {
      flex: 1,
      backgroundColor: '#E3D3E3'
    },
    layer1:{
      paddingTop:10, 
    },
    heading1:{ 
      fontSize: 20, 
      textAlign: 'center', 
      marginTop: 50, 
    }, 
    layer2:{ 
      flexDirection:'row', 
      padding:'250' , 
    }, 
    picker1: { 
      flex: 3, 
    }, 
    picker2:{ 
      flex: 2, 
    }, 
    layer3:{ 
      paddingTop:70, 
    }, 
    heading2:{ 
      marginTop: 30,
      textAlign: 'center',
      colour: '#67e487',
      fontSize: 20,
      fontWeight: 10,
    }
    });





